package arcanemod.common;

import net.minecraft.creativetab.CreativeTabs;
import net.minecraft.item.EnumToolMaterial;
import net.minecraft.item.ItemSword;

public class Tool_RedArcaneSword extends ItemSword 
{

  public Tool_RedArcaneSword(int par1, EnumToolMaterial par2EnumToolMaterial) 
	{
		super(par1, par2EnumToolMaterial);
		setItemName("RedArcaneSword");
		bFull3D = true;
		setCreativeTab(CreativeTabs.tabCombat);
		
	
		
	}
	public String getTextureFile () 
	  {
      return "/arcanemodtextures/ArcaneMod_Items.png";
    }

}
