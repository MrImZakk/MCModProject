public class Item_RedArcaneBar extends Item
{

  public Item_RedArcaneBar(int par1) 
	{
		super(par1);
		setCreativeTab(CreativeTabs.tabMaterials);
		setItemName("RedArcaneBar");
		
	}
	public String getTextureFile () 
	  {
        return "/arcanemodtextures/ArcaneMod_Items.png";
}

}
